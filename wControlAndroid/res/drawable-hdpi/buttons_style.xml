<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <gradient
                android:startColor="#fff9ffc9"
                android:endColor="#C1CC64"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#ffc1cc64" />
            <corners
                android:radius="20dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#C1CC64"
                android:endColor="#fff9ffc9"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#ffc1cc64" />
            <corners
                android:radius="20dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
</selector>